{
  "name": "nest-kysely",
  "version": "0.0.1",
  "description": "nest-kysely",
  "author": "nemmtor",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "build": "nest build",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "compose:dev": "docker compose -f docker-compose.dev.yml up -d",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./jest-e2e.config.json",
    "ci:e2e": "docker compose -f docker-compose.ci.yml up --build --exit-code-from app && docker compose -f docker-compose.ci.yml down -v --remove-orphans",
    "circular-deps": "depcruise --config .dependency-cruiser.js src"
  },
  "dependencies": {
    "@nestjs/common": "^10.2.1",
    "@nestjs/config": "^3.0.0",
    "@nestjs/core": "^10.2.1",
    "@nestjs/platform-express": "^10.2.1",
    "@nestjs/swagger": "^7.1.10",
    "@nestjs/terminus": "^10.0.1",
    "kysely": "^0.26.3",
    "pg": "^8.11.3",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.8.1",
    "zod": "^3.22.2"
  },
  "devDependencies": {
    "@golevelup/ts-jest": "^0.4.0",
    "@nestjs/cli": "^10.1.16",
    "@nestjs/schematics": "^10.0.2",
    "@nestjs/testing": "^10.2.1",
    "@types/express": "^4.17.17",
    "@types/jest": "29.5.4",
    "@types/node": "20.5.6",
    "@types/pg": "^8.10.2",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^6.4.1",
    "@typescript-eslint/parser": "^6.4.1",
    "dependency-cruiser": "^13.1.5",
    "eslint": "^8.48.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "jest": "29.6.4",
    "prettier": "^3.0.2",
    "source-map-support": "^0.5.21",
    "supertest": "^6.3.3",
    "ts-jest": "29.1.1",
    "ts-loader": "^9.4.4",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "4.2.0",
    "typescript": "^5.2.2"
  },
  "engines": {
    "node": ">=18",
    "pnpm": ">=8"
  }
}
